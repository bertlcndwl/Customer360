<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.delaware.BDW.trac2019.controller.Detail"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Detail">
				<content>
					<ObjectHeader
						title="{selectedCustomerModel>/name}"
						icon="sap-icon://customer">
						<attributes>
							<ObjectAttribute text="{selectedCustomerModel>/city}"/>
							<ObjectAttribute text="{selectedCustomerModel>/country}"/>
							<ObjectAttribute text="{selectedCustomerModel>/CustomerNumber}"/>
						</attributes>
					</ObjectHeader>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>