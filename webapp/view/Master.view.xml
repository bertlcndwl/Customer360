<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.delaware.BDW.trac2019.controller.Master"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Hello SAP World">
				<content>
					<List id="customerList" items="{customers>/customers}">
						<ObjectListItem title="{customers>name}">
						<attributes>
							<ObjectAttribute text="{Country}" />
							<ObjectAttribute text="{customers>country}" />
							<ObjectAttribute text="{customers>city}" />
						</attributes>
						</ObjectListItem>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>