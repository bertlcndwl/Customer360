<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.delaware.BDW.trac2019.controller.Master"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Customers">
				<content>
					<List id="idCustomerList"
							items="{/ZV_ZVT19_CUSTM_JL}">
								<ObjectListItem
									title="{name}"
									type="Navigation"
									press="onCustomerPress">
									<attributes>
										<ObjectAttribute title="{i18n>city}" text="{city}" />
										<ObjectAttribute title="{i18n>country}" text="{country}" />
									</attributes>
								</ObjectListItem>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>